{
  "name": "cqa",
  "version": "0.3.0",
  "description": "Command Query App",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf local && tsc",
    "build:dist": "rimraf dist && tsc -p ./tsconfig-dist.json",
    "check": "npm run build && npm run lint && jest --coverage",
    "test": "npm run build && jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint --fix src/**/*.ts",
    "preversion": "npm run check",
    "prepublishOnly": "npm run build:dist",
    "report-coverage": "minicat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwikstrom/cqa.git"
  },
  "author": "Mårten Wikström",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mwikstrom/cqa/issues"
  },
  "homepage": "https://github.com/mwikstrom/cqa#readme",
  "devDependencies": {
    "@trust/webcrypto": "^0.9.2",
    "@types/jest": "^23.3.1",
    "@types/object-hash": "^1.2.0",
    "@types/uuid": "^3.4.3",
    "babel-jest": "^23.4.2",
    "coveralls": "^3.0.2",
    "fake-indexeddb": "^2.0.4",
    "jest": "^23.5.0",
    "minicat": "^1.0.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "dexie": "^2.0.4",
    "io-ts": "^1.3.0",
    "uuid": "^3.3.2"
  }
}
