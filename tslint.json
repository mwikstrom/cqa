{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [true, "ban-single-arg-parens"],
        "max-line-length": [true, 120],
        "variable-name": [
            true,
            "allow-leading-underscore",
            "allow-pascal-case",
            "allow-snake-case",
            "ban-keywords"
        ]
    },
    "rulesDirectory": []
}